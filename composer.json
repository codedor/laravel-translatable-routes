{
    "name": "codedor/laravel-translatable-routes",
    "description": "Package for Laravel to make routes translatable",
    "keywords": [
        "codedor",
        "laravel",
        "laravel-translatable-routes"
    ],
    "homepage": "https://github.com/codedor/laravel-translatable-routes",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "codedor/laravel-locale-collection": "^1.0",
        "illuminate/contracts": "^10.0",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0",
        "larastan/larastan": "^2.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-translatable": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Codedor\\TranslatableRoutes\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Codedor\\TranslatableRoutes\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codedor\\TranslatableRoutes\\Providers\\TranslatableRoutesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
