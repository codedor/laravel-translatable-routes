{
    "name": "wotz/laravel-translatable-routes",
    "description": "Package for Laravel to make routes translatable",
    "keywords": [
        "who owns the zebra",
        "laravel",
        "laravel-translatable-routes"
    ],
    "homepage": "https://github.com/wotz/laravel-translatable-routes",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "wotz/laravel-locale-collection": "^2.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "larastan/larastan": "^2.0|^3.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0|^4.0",
        "phpstan/extension-installer": "^1.1|^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.0|^2.0",
        "phpstan/phpstan-phpunit": "^1.0|^2.0",
        "spatie/laravel-translatable": "^6.8"
    },
    "autoload": {
        "psr-4": {
            "Wotz\\TranslatableRoutes\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wotz\\TranslatableRoutes\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wotz\\TranslatableRoutes\\Providers\\TranslatableRoutesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
